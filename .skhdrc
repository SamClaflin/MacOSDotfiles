# Window focus 
cmd - left : yabai -m window --focus west
cmd - right : yabai -m window --focus east
cmd - up : yabai -m window --focus north
cmd - down : yabai -m window --focus south

# Window resize 
cmd + shift - left : yabai -m window --resize left:-40:0 ; yabai -m window --resize right:-40:0
cmd + shift - down : yabai -m window --resize bottom:0:40 ; yabai -m window --resize top:0:40
cmd + shift - up : yabai -m window --resize top:0:-40 ; yabai -m window --resize bottom:0:-40
cmd + shift - right : yabai -m window --resize right:40:0 ; yabai -m window --resize left:40:0

# Window movement 
cmd + shift - h : yabai -m window --swap west
cmd + shift - l : yabai -m window --swap east
cmd + shift - j : yabai -m window --swap south
cmd + shift - k : yabai -m window --swap north

# Space movement 
cmd + shift - 1 : yabai -m window --space 1 
cmd + shift - 2 : yabai -m window --space 2 
cmd + shift - 3 : yabai -m window --space 3 
cmd + shift - 4 : yabai -m window --space 4 
cmd + shift - 5 : yabai -m window --space 5 
cmd + shift - 6 : yabai -m window --space 6 
cmd + shift - 7 : yabai -m window --space 7 
cmd + shift - 8 : yabai -m window --space 8 

# Swap spaces between displays 
cmd + shift - m : ~/.scripts/switch_displays.py

# Close window 
cmd + shift - q : yabai -m window --close 

# Float window 
cmd + shift - f : yabai -m window --toggle float

# Open Todoist
cmd + shift - t : /Applications/Todoist.app/Contents/MacOS/Todoist

# Open Mission Control
#cmd + shift - m : /System/Applications/Mission\ Control.app/Contents/MacOS/Mission\ Control \ && ~/.scripts/set_wallpaper.py ~/Desktop/images/unet.png

# Open iTerm2
cmd - return : /Applications/iTerm.app/Contents/MacOS/iTerm2

